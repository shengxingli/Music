<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_marginTop="50dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/all_songer"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="全部"/>
        <TextView
            android:id="@+id/china"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_marginRight="10dp"
            android:text="国内"/>
        <TextView
            android:id="@+id/korea"
            android:layout_marginRight="10dp"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="韩国"/>
        <TextView
            android:id="@+id/us"
            android:layout_marginRight="10dp"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="欧美"/>
        <TextView
            android:id="@+id/japan"
            android:textSize="16dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="日本"/>
        <TextView
            android:id="@+id/other"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="其他"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="1dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/all_songer1"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="5dp"
            android:text="全部"/>
        <TextView
            android:id="@+id/famale"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="10dp"
            android:text="男"/>
        <TextView
            android:id="@+id/male"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_marginRight="10dp"
            android:text="女"/>
        <TextView
            android:id="@+id/couple"
            android:layout_marginRight="10dp"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="组合"/>
        <TextView
            android:layout_gravity="center"
            android:layout_marginRight="10dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp" />
        <TextView
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="1dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/all_style"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:text="全部"/>
        <TextView
            android:id="@+id/popuplar"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:layout_marginRight="10dp"
            android:text="流行"/>
        <TextView
            android:id="@+id/hop"
            android:layout_marginRight="10dp"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="嘻哈"/>
        <TextView
            android:id="@+id/rock"
            android:layout_marginRight="10dp"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="摇滚"/>
        <TextView
            android:id="@+id/china_style"
            android:textSize="16dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="古风"/>
        <TextView
            android:id="@+id/other_style"
            android:textSize="16dp"
            android:background="@drawable/frame_select"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="40dp"
            android:text="其他"/>
    </LinearLayout>
    <ListView
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:id="@+id/songer_item"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></ListView>
</LinearLayout>
